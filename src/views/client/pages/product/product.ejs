            <% getProduct.forEach(item =>{%>
            <div class="ps-product__column">
                <div class="ps-shoe mb-30">
                    <div class="ps-shoe__thumbnail">
                        <a class="ps-shoe__favorite" href="#"><i class="ps-icon-heart"></i></a>
                        <a class="ps-shoe__favorite add-one-item-to-cart" data-uid="<%= item._id%>"><i class="ps-icon-shopping-cart"></i></a
                        ><img src="uploads/imageproduct/<%= item.image%>" alt="" /><a
                            class="ps-shoe__overlay"
                            href="/product/detail-product/<%= item._id%>"
                        ></a>
                    </div>
                    <div class="ps-shoe__content">
                        <div class="ps-shoe__variants">
                            <div class="ps-shoe__variant normal">
                                <img src="uploads/imageproduct/<%= item.image%>" alt="" /><img
                                    src="uploads/imageproduct/<%= item.image%>"
                                    alt=""
                                />
                            </div>
                            <select class="ps-rating ps-shoe__rating">
                                <option value="1">1</option>
                                <option value="1">2</option>
                                <option value="1">3</option>
                                <option value="1">4</option>
                                <option value="2">5</option>
                            </select>
                        </div>
                        <div class="ps-shoe__detail">
                            <a class="ps-shoe__name" href="#"><%= item.title%></a>
                            <p class="ps-shoe__categories"><a href="#"><%= item.idCate.title%></a></p>
                            <%item.size.forEach((price,index) =>{%> <%if(index == 0){%>
                            <span class="ps-shoe__price"> <%= price.price%><!--<del>£220</del>--></span>
                            <%}%> <%})%>
                        </div>
                    </div>
                </div>
            </div>
            <%})%>
        </div>
        <div class="ps-product-action">
            <div class="ps-pagination">
                <ul class="pagination">
                    <%if(currentPage == 1){%>
                    <li>
                        <a><i class="fa fa-angle-left"></i></a>
                    </li>
                    <%}else{%>
                    <li>
                        <a href="/products?page=<%= currentPage -1%>"><i class="fa fa-angle-left"></i></a>
                    </li>
                    <%}%> <% var i = Number(currentPage) > 4? Number(currentPage) - 3 : 1%> <% for(;i <= (Number(currentPage) + 2) && i <=
                    pages; i++){%> <%if(i == Number(currentPage)){%>
                    <li class="active"><a><%= i%></a></li>
                    <%}else{%>
                    <li><a href="/products?page=<%= i%>"><%= i%></a></li>
                    <%}%> <%}%> <%if(currentPage == pages){%>
                    <li>
                        <a href="#" class="disabled display-none"><i class="fa fa-angle-right"></i></a>
                    </li>
                    <%}else{%>
                    <li>
                        <a href="/products?page=<%= Number(currentPage) + 1%>"><i class="fa fa-angle-right"></i></a>
                    </li>
                    <%}%>
                </ul>
            </div>
        </div>
